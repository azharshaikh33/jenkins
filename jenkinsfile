    agent any

    stages {
        stage('Stage Name - 1') {
            steps {
                sh 'echo This is my first stage in the Jenkins pipeline'
            }
        }

        stage('Stage Name - 2') {
            steps {
                sh 'echo executing stage 2'
            }
        }

        stage('Stage Name - 3') {
            steps {
                sh '''

                echo i am learning jenkins
                lets learn to the core
                well it seems easy
                
                '''
            }
        }
    }
}
